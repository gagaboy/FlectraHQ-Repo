                            </tr>
                            <tr>
                                <td><label for="paper_format">Paper Format</label></td>
                                <td class="web_sidebar_text">
                                    <select id="paperformat_id" name="paper_format" class="paper_m2m_select2"/>
                                </td>
                            </tr>
                            <tr>
                                <td><label for="security_groups">Security Groups</label></td>
                                <td class="web_sidebar_text">
                                    <input id="groups_id" name="security_groups" type="text" class="group_m2m_select2" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </t>

    <t t-name="web.ActionViewEditorSidebar">
        <div id="web_report_container" class="web_report_sidebar">
            <div class="web_report_body">
                <div class="web_report_sidebar_content">
                    <table class="table table-hover">
                        <tbody>
                            <tr>
                                <td><label for="string">Title</label></td>
                                <td class="web_sidebar_text"><input id="name" name="string" type="text" t-att-value="widget.attrs.name"/></td>
                            </tr>
