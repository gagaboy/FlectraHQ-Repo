    <t t-name="web.reportSideBar">
        <div id="web_report_container" class="web_report_sidebar">
            <div class="web_report_body">
                <div class="web_report_sidebar_content">
                    <table class="table table-hover">
                        <tbody>
                            <tr class="web_sidebar_checkbox">
                                <td><label for="id_pmenu">Add in Print Menu</label></td>
                                <td class="o_checkbox"><input id="id_pmenu" type="checkbox"/></td>
                            </tr>
                            <tr>
                                <td><label for="string">Name</label></td>
                                <td class="web_sidebar_text">
                                    <input id="name" name="string" type="text" value=""/>
